#include "holberton.h"
/**
 * more_numbers - prints 10 times the numbers, from 0 to 14, followed new line
 * Return: Always 0
 */
void more_numbers(void)
{
	int time = 1;
	int dig1;
	int dig2;

	do
	{
		for (dig1 = 48; dig1 <= 57; dig1++)
		{
			_putchar(dig1);
		}
		for (dig2 = 48; dig2 < 53; dig2++)
		{
			_putchar(49);
			_putchar(dig2);
		}
		_putchar('\n');
		time++;
	}
	while (time <= 10);
}
